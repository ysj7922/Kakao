<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanbit.kakao">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.member.RegistMemberActivity" />
        <activity android:name=".presentation.member.MemberListActivity" />
        <activity android:name=".presentation.member.LoginActivity" />
        <activity android:name=".presentation.member.ModifyMemberActivity" />
        <activity android:name=".presentation.member.SearchMemberActivity" />
        <activity android:name=".presentation.member.UnregistMemberActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.message.WriteActivity" />
        <activity android:name=".IntroActivity"></activity>
    </application>

</manifest>